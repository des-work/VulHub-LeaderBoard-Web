# VulHub Frontend - Environment Variables
# Copy this file to .env.local and update values as needed

# API Endpoint (where the backend is running)
# For local development, this should be your local API
NEXT_PUBLIC_API_URL=http://localhost:4010/api/v1

# Environment
NODE_ENV=development

# Optional: App Name
NEXT_PUBLIC_APP_NAME=VulHub Leaderboard
